void quantidade_elementos(estrutura_pilha pilha) {
    pilha.topo++;
    printf("NÃºmero de elementos: %d\n", pilha.topo);
}

void imprimir(estrutura_pilha pilha) 
{
int i;
printf ("Elementos:\n");
	for (i=pilha.topo; i>=0; i--)
	{
	printf ("%i\n", pilha.elem[i]); 
	}
}

int main(){
  estrutura_pilha pilha;
  pilha.elem[0] = 1;
  pilha.elem[1] = 2;
  pilha.elem[2] = 3;
  pilha.elem[3] = 4;
  pilha.elem[4] = 5;
  pilha.topo = 4;
  pilha.chave = 5;
  imprimir(pilha);
  quantidade_elementos(pilha);
  return 0;
}
